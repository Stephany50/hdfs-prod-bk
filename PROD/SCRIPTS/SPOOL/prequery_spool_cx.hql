SELECT IF(T_1.SPOOL_COUNT = 0 AND T_2.IT_COUNT > 1 AND T_3.FT_COUNT > 1,"OK","NOK") FT_EXIST
FROM
    (SELECT COUNT(*) SPOOL_COUNT FROM SPOOL.SPOOL_CX WHERE EVENT_DATE='###SLICE_VALUE###') T_1,
    (SELECT COUNT(*) IT_COUNT FROM CDR.SPARK_IT_OMNY_TRANSACTIONS WHERE TRANSFER_DATETIME='###SLICE_VALUE###') T_2,
    (SELECT COUNT(*) FT_COUNT FROM MON.SPARK_FT_CONTRACT_SNAPSHOT WHERE EVENT_DATE='###SLICE_VALUE###') T_3
