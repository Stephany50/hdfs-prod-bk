CREATE TABLE SPOOL.SPOOL_DATA_VIA_OM
(
    BENEFICIAIRE VARCHAR(300),
    NOM_BENEFICIAIRE VARCHAR(300),
    PRENOM_BENEFICIAIRE VARCHAR(300),
    BUNDLE_NAME VARCHAR(300),
    BDLE_COST DECIMAL(17, 2),
    NBER_PURCHASE BIGINT,
    INSERT_DATE TIMESTAMP
)
PARTITIONED BY (TRANSACTION_DATE DATE)
STORED AS PARQUET TBLPROPERTIES ('PARQUET.COMPRESS'='SNAPPY')
