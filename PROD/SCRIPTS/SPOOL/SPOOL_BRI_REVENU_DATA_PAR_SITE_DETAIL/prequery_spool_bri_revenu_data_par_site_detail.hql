SELECT IF(T_1.SPOOL_COUNT = 0 AND T_2.FT_COUNTA > 0 ,"OK","NOK") FT_EXIST
FROM
    (SELECT COUNT(*) SPOOL_COUNT FROM SPOOL.SPOOL_BRI_REVENU_DATA_PAR_SITE_DETAIL WHERE BASE_MONTH ='###SLICE_VALUE###') T_1,
    (SELECT COUNT(*) FT_COUNTA FROM MON.SPARK_TF_BASE_GROUP_CONSO_MONTH WHERE BASE_MONTH ='###SLICE_VALUE###') T_2

