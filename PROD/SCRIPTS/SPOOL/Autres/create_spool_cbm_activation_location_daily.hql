CREATE TABLE SPOOL.SPOOL_CBM_ACTIVATION_LOCATION_DAILY
(
    MSISDN VARCHAR(100),
    ACCOUNT_FORMULE VARCHAR(100),
    OSP_CONTRACT_TYPE VARCHAR(100),
    FIRST_SITE_NAME VARCHAR(100),
    TOWNNAME VARCHAR(100),
    ADMINISTRATIVE_REGION VARCHAR(100),
    CATEGORIES_SITE VARCHAR(100),
    INSERT_DATE TIMESTAMP
)
PARTITIONED BY (EVENT_DATE DATE)
STORED AS PARQUET TBLPROPERTIES ('PARQUET.COMPRESS'='SNAPPY')