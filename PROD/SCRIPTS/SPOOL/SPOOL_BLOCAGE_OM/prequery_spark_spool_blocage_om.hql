SELECT IF(T_1.SPOOL_COUNT = 0 AND
          A.nb_bal >= 10 AND B.nb_bar >= 10,"OK","NOK") FT_EXIST
FROM
    (SELECT COUNT(*) SPOOL_COUNT FROM SPOOL.SPARK_SPOOL_BLOCAGE_OM WHERE EVENT_DATE='###SLICE_VALUE###') T_1,
    (select count(*) nb_bar from CDR.SPARK_IT_OMNY_BARRED_ACCOUNTS where ORIGINAL_FILE_DATE='###SLICE_VALUE###') A
    (select count(*) nb_om from MON.SPARK_FT_OMNY_ACCOUNT_SNAPSHOT where EVENT_DATE='###SLICE_VALUE###') B

