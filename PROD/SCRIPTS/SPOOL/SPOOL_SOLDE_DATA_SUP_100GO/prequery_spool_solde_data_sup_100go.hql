SELECT IF(T_1.SPOOL_COUNT = 0 AND T_2.IT_COUNT > 0 AND T_3.FT_COUNT > 0,"OK","NOK") FT_EXIST
FROM
    (SELECT COUNT(*) SPOOL_COUNT FROM SPOOL.SPOOL_SOLDE_DATA_SUP_100GO WHERE EVENT_DATE='###SLICE_VALUE###') T_1,
    (SELECT COUNT(*) IT_COUNT FROM CDR.SPARK_IT_ZTE_SUBS_EXTRACT WHERE ORIGINAL_FILE='###SLICE_VALUE###') T_2,
    (SELECT COUNT(*) FT_COUNT FROM CDR.SPARK_IT_ZTE_BAL_SNAP WHERE ORIGINAL_FILE_DATE='###SLICE_VALUE###') T_3