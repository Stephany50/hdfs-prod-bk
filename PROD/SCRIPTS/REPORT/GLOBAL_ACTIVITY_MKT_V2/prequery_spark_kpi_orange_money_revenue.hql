SELECT IF(T1.KPI_IS_LOAD=0 AND OM_MARK>0,'OK','NOK')
 FROM
(SELECT COUNT(*) KPI_IS_LOAD FROM AGG.SPARK_FT_GLOBAL_ACTIVITY_DAILY_MKT_DG  WHERE TRANSACTION_DATE='###SLICE_VALUE###' AND KPI IN ('REVENUE_OM'))T1,
(SELECT COUNT(*) OM_MARK FROM AGG.SPARK_FT_A_DATAMART_OM_MARKETING WHERE jour ='###SLICE_VALUE###')T2