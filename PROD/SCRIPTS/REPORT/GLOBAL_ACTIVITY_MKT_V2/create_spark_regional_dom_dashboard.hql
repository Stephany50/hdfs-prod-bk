
create table AGG.SPARK_REGIONAL_DOM_DASHBOARD (
    POSITION  VARCHAR(100),
    DESCRIPTION  VARCHAR(100),
    REGION  VARCHAR(100),
    vol  BIGINT,
    val DOUBLE,
    REVENU DOUBLE,
    COMM DOUBLE,
    FRAIS DOUBLE
)PARTITIONED BY (    JOUR DATE)
STORED AS PARQUET
TBLPROPERTIES ('PARQUET.COMPRESS'='SNAPPY')
