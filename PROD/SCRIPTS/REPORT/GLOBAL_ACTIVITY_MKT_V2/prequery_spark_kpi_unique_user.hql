
SELECT IF(T1.KPI_IS_LOAD=0 AND T2.FT_USERS_DAY>0 AND T3.FT_USERS_DATA_DAY>0,'OK','NOK')
 FROM
(SELECT COUNT(*) KPI_IS_LOAD FROM AGG.SPARK_FT_GLOBAL_ACTIVITY_DAILY_MKT_DG2  WHERE TRANSACTION_DATE='###SLICE_VALUE###' AND SOURCE_TABLE IN ('FT_USERS_DAY','FT_USERS_DATA_DAY'))T1,
(SELECT COUNT(*) FT_USERS_DAY FROM MON.SPARK_FT_USERS_DAY WHERE EVENT_DATE='###SLICE_VALUE###')T2,
(SELECT COUNT(*) FT_USERS_DATA_DAY FROM MON.SPARK_FT_USERS_DATA_DAY WHERE EVENT_DATE='###SLICE_VALUE###')T3
