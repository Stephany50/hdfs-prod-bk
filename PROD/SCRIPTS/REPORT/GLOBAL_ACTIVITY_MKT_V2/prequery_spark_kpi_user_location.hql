SELECT IF(T1.KPI_IS_LOAD=0 AND T2.FT_EXISTS>0,'OK','NOK')
 FROM
(SELECT COUNT(*) KPI_IS_LOAD FROM AGG.SPARK_FT_GLOBAL_ACTIVITY_DAILY_MKT_DG  WHERE TRANSACTION_DATE='###SLICE_VALUE###' AND JOB_NAME='COMPUTE_KPI_USER_LOCATION')T1,
(SELECT COUNT(*) FT_EXISTS FROM MON.SPARK_FT_USERS_REGION_LOCATION WHERE EVENT_DATE='###SLICE_VALUE###')T2
