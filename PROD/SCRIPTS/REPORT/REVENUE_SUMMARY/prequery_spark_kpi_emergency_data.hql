SELECT IF(T_1.FT_EXIST = 0 and T_2.FT_EMERGENCY_DATA_EXIST>0 ,"OK","NOK") EXTRACT_EXIST
FROM
(SELECT COUNT(*) FT_EXIST FROM AGG.SPARK_REVENUE_SUMMARY_DAILY WHERE EVENT_DATE='###SLICE_VALUE###' and source_data='FT_EMERGENCY_DATA') T_1,
(SELECT COUNT(*) FT_EMERGENCY_DATA_EXIST FROM MON.SPARK_FT_EMERGENCY_DATA WHERE TRANSACTION_DATE = '###SLICE_VALUE###') T_2


