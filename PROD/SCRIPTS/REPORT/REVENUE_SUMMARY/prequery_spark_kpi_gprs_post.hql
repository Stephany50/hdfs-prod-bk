SELECT IF(T_1.FT_EXIST = 0 and T_2.FT_A_GPRS_ACTIVITY_POST_EXIST>0 ,"OK","NOK") EXTRACT_EXIST
FROM
(SELECT COUNT(*) FT_EXIST FROM AGG.SPARK_REVENUE_SUMMARY_DAILY WHERE EVENT_DATE='###SLICE_VALUE###' AND source_data='FT_A_GPRS_ACTIVITY_POST') T_1,
(SELECT COUNT(*) FT_A_GPRS_ACTIVITY_POST_EXIST FROM AGG.SPARK_FT_A_GPRS_ACTIVITY_POST WHERE DATECODE = '###SLICE_VALUE###') T_2