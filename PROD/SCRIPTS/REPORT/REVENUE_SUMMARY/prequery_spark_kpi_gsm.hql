SELECT IF(T_1.FT_EXIST = 0 and T_2.FT_GSM_TRAFFIC_REVENUE_DAILY_EXIST>0 ,"OK","NOK") EXTRACT_EXIST
FROM
(SELECT COUNT(*) FT_EXIST FROM AGG.SPARK_REVENUE_SUMMARY_DAILY WHERE EVENT_DATE='###SLICE_VALUE###' AND source_data='FT_GSM_TRAFFIC_REVENUE_DAILY') T_1,
(SELECT COUNT(*) FT_GSM_TRAFFIC_REVENUE_DAILY_EXIST FROM AGG.SPARK_FT_GSM_TRAFFIC_REVENUE_DAILY WHERE TRANSACTION_DATE = '###SLICE_VALUE###') T_2