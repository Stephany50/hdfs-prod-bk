SELECT IF(T_1.FT_EXIST = 0 and T_2.FT_EXIST > 0 ,"OK","NOK") STATUT
FROM
(SELECT COUNT(*) FT_EXIST FROM MON.SPARK_FT_VOICE_SMS_USERS WHERE TRANSACTION_DATE='###SLICE_VALUE###') T_1,
(SELECT COUNT(*) FT_EXIST FROM MON.SPARK_FT_BILLED_TRANSACTION_PREPAID WHERE TRANSACTION_DATE = '###SLICE_VALUE###') T_2
-- (SELECT COUNT(DISTINCT TRANSACTION_DATE) COMPLETUDE FROM MON.SPARK_FT_BILLED_TRANSACTION_PREPAID WHERE TRANSACTION_DATE BETWEEN DATE_SUB('###SLICE_VALUE###',29) AND '###SLICE_VALUE###' ) T_2