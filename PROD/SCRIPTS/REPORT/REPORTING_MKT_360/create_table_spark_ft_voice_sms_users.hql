CREATE TABLE MON.SPARK_FT_VOICE_SMS_USERS (
    MSISDN VARCHAR(40),
    NB_VOICE_DAILY BIGINT,
    NB_VOICE_7_DAYS BIGINT,
    NB_VOICE_30_DAYS BIGINT,
    NB_VOICE_MTD BIGINT,
    NB_SMS_DAILY BIGINT,
    NB_SMS_7_DAYS BIGINT,
    NB_SMS_30_DAYS BIGINT,
    NB_SMS_MTD BIGINT,
    TRAFFIC_VOICE_DAILY BIGINT,
    TRAFFIC_VOICE_7_DAYS BIGINT,
    TRAFFIC_VOICE_30_DAYS BIGINT,
    TRAFFIC_VOICE_MTD BIGINT,
    INSERT_DATE TIMESTAMP
)
PARTITIONED BY (TRANSACTION_DATE DATE)
STORED AS PARQUET
TBLPROPERTIES ("parquet.compress"="SNAPPY")