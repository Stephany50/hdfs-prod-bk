
CREATE TABLE MON.SPARK_FT_DAILY_STATUS(
    TABLE_TYPE STRING,
    TABLE_SOURCE STRING,
    TABLE_NAME STRING,
    DEPENDANCES STRING,
    STATUT STRING,
    NB_ROWS INT,
    LAST_INCIDENT_DAY TIMESTAMP,
    TABLE_INSERT_DATE TIMESTAMP,
    INSERT_DATE TIMESTAMP
)
PARTITIONED BY (TABLE_DATE DATE)
STORED AS PARQUET TBLPROPERTIES ('PARQUET.COMPRESS'='SNAPPY')
