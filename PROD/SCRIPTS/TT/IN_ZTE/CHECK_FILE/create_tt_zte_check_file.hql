CREATE EXTERNAL TABLE IF NOT EXISTS CDR.TT_ZTE_CHECK_FILE (
    CDR_NAME VARCHAR (100),
    CDR_GEN_DATE TIMESTAMP,
    CDR_FILE_SIZE INT,
    CDR_COUNT INT,
    ORIGINAL_FILE_NAME VARCHAR (100)
