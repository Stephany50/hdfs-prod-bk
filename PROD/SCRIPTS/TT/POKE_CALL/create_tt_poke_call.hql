
CREATE EXTERNAL TABLE IF NOT EXISTS CDR.TT_POKE_CALL (
  CARDNUMBER VARCHAR(100),
  SERVICEKEY VARCHAR(100),
  CALLINGNUMBER VARCHAR(100),
  CALLEDNUMBER VARCHAR(100),
  SETUPTIME VARCHAR(100),
  TERMINATETIME VARCHAR(100),
  RESULT VARCHAR(100),
  CAUSEVALUE VARCHAR(100)
)
COMMENT 'POKE external tables-TT'
ROW FORMAT DELIMITED FIELDS TERMINATED BY '|'
LOCATION '/PROD/TT/POKE_CALL/'
TBLPROPERTIES ('serialization.null.format'='')

