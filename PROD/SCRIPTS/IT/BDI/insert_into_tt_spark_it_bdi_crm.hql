INSERT INTO CDR.IT_BDI_HLR PARTITION (FILE_DATE)
SELECT
IMSI ,
MSISDN ,
ODBIC ,
ODBOC ,
ODBPLMN1 ,
ODBPLMN2 ,
ODBPLMN3 ,
ODBPLMN4 ,
ODBROAM ,
ODBDECT ,
ODBMECT ,
ODBINFO ,
ODBECT ,
ODBPOS ,
ODBPOSTYPE ,
ODBRCF ,
ODBENTER ,
ODBSS ,
CURRENT_TIMESTAMP() INSERT_DATE,
TO_DATE(FROM_UNIXTIME(UNIX_TIMESTAMP(SUBSTRING ('MemExpFile_HSS9860_1200_70_20200313085335.txt', -18, 8),'yyyyMMdd'))) FILE_DATE
FROM CDR.TT_bdi_hlr_copy C
LEFT JOIN (SELECT DISTINCT  ORIGINAL_FILE_NAME FILE_NAME FROM CDR.SPARK_IT_BDI WHERE ORIGINAL_FILE_DATE BETWEEN DATE_SUB(CURRENT_DATE,${hivevar:date_offset}) AND CURRENT_DATE )T ON T.FILE_NAME=C.ORIGINAL_FILE_NAME
WHERE T.FILE_NAME IS NULL
