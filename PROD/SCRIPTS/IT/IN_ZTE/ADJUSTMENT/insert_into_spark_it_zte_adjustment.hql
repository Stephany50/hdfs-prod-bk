INSERT INTO CDR.SPARK_IT_ZTE_ADJUSTMENT PARTITION (CREATE_DATE,FILE_DATE)
SELECT
  ACCT_CODE,
  ACC_NBR,
  ACCT_BOOK_ID,
  ACCT_RES_CODE,
  substr(pre_exp_date, 1, 19) PRE_REAL_BALANCE,
  CHARGE,
  PRE_EXP_DATE,
  DAYS,
  CHANNEL_ID,
  CREATE_DATE NQ_CREATE_DATE,
  TRANSACTIONSN,
  PROVIDER_ID,
  PREPAY_FLAG ,
  LOAN_AMOUNT,
  COMMISSION_AMOUNT,
  ORIGINAL_FILE_NAME,
  ORIGINAL_FILE_SIZE,
  ORIGINAL_FILE_LINE_COUNT,
  TO_DATE(FROM_UNIXTIME(UNIX_TIMESTAMP(SUBSTRING (ORIGINAL_FILE_NAME, -19, 8),'yyyyMMdd'))) ORIGINAL_FILE_DATE,
  CURRENT_TIMESTAMP() INSERT_DATE,
  PARTY_TYPE,
  STAFF_NAME,
  USER_CODE,
  LIST_BAL_ID,
  PRE_REAL_BALANCE PRE_REAL_BALANCE_NEW,
  TO_DATE(FROM_UNIXTIME(UNIX_TIMESTAMP(SUBSTRING (CREATE_DATE, 1, 10),'yyyy-MM-dd'))) CREATE_DATE,
  TO_DATE(FROM_UNIXTIME(UNIX_TIMESTAMP(SUBSTRING (ORIGINAL_FILE_NAME, -19, 8),'yyyyMMdd'))) FILE_DATE
FROM CDR.TT_ZTE_ADJUSTMENT C
LEFT JOIN (SELECT DISTINCT  ORIGINAL_FILE_NAME FILE_NAME FROM CDR.SPARK_IT_ZTE_ADJUSTMENT WHERE CREATE_DATE BETWEEN DATE_SUB(CURRENT_DATE,${hivevar:date_offset}) AND CURRENT_DATE )T ON T.FILE_NAME=C.ORIGINAL_FILE_NAME
WHERE T.FILE_NAME IS NULL