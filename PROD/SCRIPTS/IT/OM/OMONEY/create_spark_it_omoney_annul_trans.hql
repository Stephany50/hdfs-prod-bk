CREATE TABLE CDR.SPARK_IT_OMONEY_ANNUL_TRANS
(
    Id VARCHAR(250),
    SERVICE VARCHAR(250),
    TRANSACTION_STATUS VARCHAR(250),
    MSISDN_PAYER VARCHAR(250),
    MSISDN_PAYEE VARCHAR(250),
    AMOUNT VARCHAR(250),
