    TRANSACTION_ID_CORRECTION VARCHAR(250),
    TRANSFERDATE timestamp,
    DATE_INITIATION timestamp,
    DATE_REJECT timestamp,
    DATE_APPROVAL timestamp,
    ORIGINAL_FILE_NAME  VARCHAR(50),
