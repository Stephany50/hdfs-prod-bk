SELECT
    EVENT_DATE PERIOD,
    IF(SUBSTR(TRIM(CAST(IDCUST AS STRING)), 1, 3) = 237, SUBSTR(TRIM(CAST(IDCUST AS STRING)), 4, 9), TRIM(CAST(IDCUST AS STRING))) MSISDN,
    SERVPROVIDER SITE_VISITED
FROM CDR.SPARK_IT_CUSTOMER_SP_OTARIE
WHERE EVENT_DATE = '###SLICE_VALUE###'
    AND LENGTH(CAST(IDCUST AS STRING))<=12
    AND IDCUST <> '0'