SELECT IF(
T_1.IT_CONT > 0 AND
T_3.NB_EXPORT < 1
,"OK","NOK")
FROM
(SELECT COUNT(*) IT_CONT FROM CDR.SPARK_IT_CONT WHERE ORIGINAL_FILE_DATE = '###SLICE_VALUE###' ) T_1,
(
select count(*) NB_EXPORT from
(select * from (SELECT * FROM MON.EXPORT_HISTORY where event_date='###SLICE_VALUE###' and JOB_INSTANCEID='LOAD_EXPORT_MAPPING_MSISDN_IMSI' ORDER BY INSERT_DATE DESC LIMIT 1)  T where T.STATUS = 'OK') M
) T_3
