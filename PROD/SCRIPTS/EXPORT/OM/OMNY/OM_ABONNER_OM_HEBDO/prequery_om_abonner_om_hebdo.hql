SELECT IF(
T_1.CDR_EXIST > 6 AND
T_2.NB_EXPORT < 1
,"OK","NOK")
FROM
(SELECT COUNT(DISTINCT MODIFICATION_DATE) CDR_EXIST FROM CDR.SPARK_IT_OM_SUBSCRIBERS  WHERE MODIFICATION_DATE BETWEEN (date_sub('###SLICE_VALUE###',pmod(datediff('###SLICE_VALUE###','1900-01-01'),7))) AND (date_add('###SLICE_VALUE###',6 - pmod(datediff('###SLICE_VALUE###','1900-01-01'),7)))) T_1,
(
select count(*) NB_EXPORT from
(select * from (SELECT * FROM MON.EXPORT_HISTORY where event_date=date_sub('###SLICE_VALUE###',pmod(datediff('###SLICE_VALUE###','1900-01-01'),7)) and JOB_INSTANCEID='LOAD_OM_ABONNER_OM_HEBDO' ORDER BY INSERT_DATE DESC LIMIT 1)  T where T.STATUS = 'OK') M
) T_2