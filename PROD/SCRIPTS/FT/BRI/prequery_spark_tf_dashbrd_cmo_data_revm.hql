SELECT IF(A.FT_COUNT > 0 and B.FT_COUNT>0 and C.FT_COUNT>0 and D.FT_COUNT=0 , "OK","NOK") FROM
(SELECT COUNT(*) FT_COUNT FROM MON.SPARK_FT_CLIENT_LAST_SITE_LOCATION WHERE EVENT_MONTH='###SLICE_VALUE###') A,
(SELECT COUNT(*) FT_COUNT FROM MON.SPARK_FT_IMEI_TRAFFIC_MONTHLY WHERE SMONTH=date_format(ADD_MONTHS('###SLICE_VALUE###' ||  '-01' , 0),'yyyyMM')) B,
(SELECT COUNT(*) FT_COUNT FROM MON.SPARK_FT_DATA_CONSO_MSISDN_MONTH WHERE EVENT_MONTH='###SLICE_VALUE###') C,
(SELECT COUNT(*) FT_COUNT FROM MON.SPARK_TF_DASHBRD_CMO_DATA_REVM WHERE EVENT_MONTH='###SLICE_VALUE###') D
