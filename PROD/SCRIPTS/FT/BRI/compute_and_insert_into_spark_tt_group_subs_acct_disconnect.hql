INSERT INTO MON.SPARK_TT_GROUP_SUBS_ACCT_DISCONNECT
SELECT b.MSISDN,
b.OG_CALL,
b.IC_CALL_1,
b.IC_CALL_2,
b.IC_CALL_3,
b.IC_CALL_4,
b.STATUS,
b.GP_STATUS,
b.GP_STATUS_DATE,
b.GP_FIRST_ACTIVE_DATE,
b.ACTIVATION_DATE,
b.RESILIATION_DATE,
b.PROVISION_DATE,
b.FORMULE,
b.PLATFORM_STATUS,
b.REMAIN_CREDIT_MAIN,
b.REMAIN_CREDIT_PROMO,
b.LANGUAGE_ACC,
b.SRC_TABLE,
b.CONTRACT_ID,
b.CUSTOMER_ID,
b.ACCOUNT_ID,
b.LOGIN,
b.ICC_COMM_OFFER,
b.BSCS_COMM_OFFER,
b.BSCS_STATUS,
b.OSP_ACCOUNT_TYPE,
b.CUST_GROUP,
b.CUST_BILLCYCLE,
b.BSCS_STATUS_DATE,
b.INACTIVITY_BEGIN_DATE,
b.COMGP_STATUS,
b.COMGP_STATUS_DATE,
b.COMGP_FIRST_ACTIVE_DATE,
current_timestamp INSERT_DATE,
to_date(b.EVENT_DATE)
        FROM ((SELECT * FROM MON.SPARK_FT_ACCOUNT_ACTIVITY where EVENT_DATE=CONCAT(SUBSTR ('###SLICE_VALUE###', 1,7 ),'-01'))) a
            , ((SELECT * FROM MON.SPARK_FT_ACCOUNT_ACTIVITY where EVENT_DATE=date_sub(last_day('###SLICE_VALUE###'),-1))) b
        WHERE a.msisdn=b.msisdn
        AND NVL(a.GP_STATUS,'ND')='ACTIF' AND NVL(b.GP_STATUS,'ND')='INACT'