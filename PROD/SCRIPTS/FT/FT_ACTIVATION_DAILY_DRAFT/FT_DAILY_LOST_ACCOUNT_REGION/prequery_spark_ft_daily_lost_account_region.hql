SELECT IF(A.FT_EXIST = 0 and B.FT_COUNT>0 and C.FT_COUNT>0 , "OK","NOK") FROM
(SELECT COUNT(*) FT_EXIST FROM MON.SPARK_FT_DAILY_LOST_ACCOUNT_REGION WHERE EVENT_DATE="###SLICE_VALUE###") A,
(SELECT COUNT(*) FT_COUNT FROM MON.SPARK_FT_ACCOUNT_ACTIVITY WHERE EVENT_DATE = DATE_ADD("###SLICE_VALUE###",1)) B,
(SELECT COUNT(*) FT_COUNT FROM MON.SPARK_FT_CLIENT_LAST_SITE_DAY WHERE EVENT_DATE="###SLICE_VALUE###") C
