SELECT IF(T_1.FT_EXIST = 0 and T_2.FT_EXIST_1>0 and T_3.FT_EXIST_2 >0 ,"OK","NOK") FT_IMEI_ONLINE_EXIST
FROM
(SELECT COUNT(*) FT_EXIST FROM MON.SPARK_FT_IMEI_ONLINE_ACTIVE WHERE EVENT_DATE = '###SLICE_VALUE###' ) T_1,
(SELECT COUNT(*) FT_EXIST_1 FROM MON.SPARK_FT_IMEI_ONLINE_ACTIVE WHERE EVENT_DATE = DATE_SUB('###SLICE_VALUE###',1)) T_2,
(SELECT COUNT(*) FT_EXIST_2 FROM MON.SPARK_FT_IMEI_ONLINE WHERE SDATE = '###SLICE_VALUE###' ) T_3
