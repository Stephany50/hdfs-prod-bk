SELECT IF(T_1.FT_EXIST = 0 and T_2.FT_ED > 0 AND T_3.FT_EC > 0  AND T_4.FT_CLSD > 0 AND T_5.IT_PPE > 0,"OK","NOK") FT_SOS_ORANGE
FROM
(SELECT COUNT(*) FT_EXIST FROM MON.SPARK_FT_SOS_ORANGE_REPORTS WHERE EVENT_DATE = date_sub('###SLICE_VALUE###',1)  ) T_1,
(SELECT COUNT(*) FT_ED FROM MON.SPARK_FT_EMERGENCY_DATA  WHERE TRANSACTION_DATE = '###SLICE_VALUE###') T_2,
(SELECT COUNT(*) FT_EC FROM MON.SPARK_FT_OVERDRAFT  WHERE TRANSACTION_DATE = '###SLICE_VALUE###') T_3,
(SELECT COUNT(*) FT_CLSD FROM MON.SPARK_FT_CLIENT_LAST_SITE_DAY  WHERE EVENT_DATE = '###SLICE_VALUE###') T_4,
(SELECT COUNT(*) IT_PPE FROM CDR.SPARK_IT_ZTE_PRICE_PLAN_EXTRACT  WHERE ORIGINAL_FILE_DATE = '###SLICE_VALUE###') T_5,
(SELECT COUNT(*) IT_PPE FROM CDR.SPARK_IT_ZTE_LOAN_CDR  WHERE ORIGINAL_FILE_DATE = '###SLICE_VALUE###') T_5

