SELECT IF(T_1.FT_EXIST = 0 and T_2.IT_TABLE > 0 AND T_3.IT_PRICE_PLAN > 0 ,"OK","NOK") FT_IMEI_ONLINE_EXIST
FROM
(SELECT COUNT(*) FT_EXIST FROM MON.SPARK_FT_SOS_VOICE WHERE TRANSACTION_DATE = date_sub('###SLICE_VALUE###',1)  ) T_1,
(SELECT COUNT(*) IT_TABLE FROM CDR.SPARK_IT_ZTE_LOAN_CDR  WHERE TRANSACTION_DATE = '###SLICE_VALUE###') T_2,
(SELECT COUNT(*) IT_PRICE_PLAN FROM CDR.SPARK_IT_ZTE_PRICE_PLAN_EXTRACT  WHERE ORIGINAL_FILE_DATE = '###SLICE_VALUE###') T_3

