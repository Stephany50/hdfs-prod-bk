SELECT IF( FT_CONSO_MSISDN_DAY=31 AND FT_USERS_DAY=0, 'OK','NOK') STATUT FROM
(SELECT COUNT(*) FT_USERS_DAY FROM MON.SPARK_FT_USERS_DAY WHERE EVENT_DATE='###SLICE_VALUE###') A,
(SELECT COUNT(distinct event_date) FT_CONSO_MSISDN_DAY FROM MON.SPARK_FT_CONSO_MSISDN_DAY WHERE EVENT_DATE between date_sub('###SLICE_VALUE###', 30) and '###SLICE_VALUE###') B
