SELECT IF(T1.KPI_IS_LOAD=0 AND T2.FT_EXISTS>0,'OK','NOK')
 FROM
(SELECT COUNT(*) KPI_IS_LOAD FROM AGG.FT_GLOBAL_ACTIVITY_DAILY  WHERE TRANSACTION_DATE='###SLICE_VALUE###' AND SOURCE_DATA='FT_EMERGENCY_DATA')T1,
(SELECT COUNT(*) FT_EXISTS FROM MON.FT_EMERGENCY_DATA WHERE TRANSACTION_DATE='###SLICE_VALUE###')T2