SELECT IF(FT_A_EXISTS=0 AND FT_EXISTS>0 AND T3.NB_INSERT_DATE= 1,'OK','NOK' )
FROM
(SELECT COUNT(*) FT_A_EXISTS FROM AGG.SPARK_FT_A_CREDIT_TRANSFER_REVENUE WHERE REFILL_DATE='###SLICE_VALUE###')T1,
(SELECT COUNT(*) FT_EXISTS FROM MON.SPARK_FT_CREDIT_TRANSFER WHERE REFILL_DATE='###SLICE_VALUE###')T2,
(SELECT COUNT(distinct insert_date) NB_INSERT_DATE FROM MON.SPARK_FT_CREDIT_TRANSFER WHERE REFILL_DATE='###SLICE_VALUE###')T3