SELECT IF(A.FT_ACTIV_EXIST = 0  AND C.FT_SUB_EXISTS >0,"OK","NOK") FROM
(SELECT count(*) FT_ACTIV_EXIST FROM MON.SPARK_FT_ACTIVATION_BY_IDENTIF_DAY  WHERE ACTIVATION_DATE='###SLICE_VALUE###') A,
(SELECT COUNT(*) FT_SUB_EXISTS FROM MON.SPARK_FT_SUBSCRIPTION WHERE TRANSACTION_DATE='###SLICE_VALUE###')C

