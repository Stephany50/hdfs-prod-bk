SELECT IF(T_1.FT_COUNT = 0 AND T_2.nb_convertbalance > 1 AND T_3.nb_adjustment > 1,"OK","NOK") FT_EXIST
FROM
    (SELECT COUNT(*) FT_COUNT FROM MON.SPARK_FT_CBM_BUNDLEX WHERE CREATEDDATE="###SLICE_VALUE###") T_1,
    (SELECT COUNT(*) nb_convertbalance FROM CDR.SPARK_IT_CONVERTBALANCE WHERE CREATEDDATE="###SLICE_VALUE###") T_2,
    (SELECT COUNT(*) nb_adjustment FROM CDR.SPARK_IT_ZTE_ADJUSTMENT WHERE create_date="###SLICE_VALUE###") T_3