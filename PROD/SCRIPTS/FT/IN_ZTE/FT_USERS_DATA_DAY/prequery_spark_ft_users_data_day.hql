SELECT IF(FT_DATA_CONSO_MSISDN_DAY=30  AND FT_USERS_DATA_DAY=0, 'OK','NOK') STATUT FROM
(SELECT COUNT(*) FT_USERS_DATA_DAY FROM MON.SPARK_FT_USERS_DATA_DAY WHERE EVENT_DATE='###SLICE_VALUE###') A,
(SELECT COUNT(distinct event_date) FT_DATA_CONSO_MSISDN_DAY FROM MON.SPARK_FT_DATA_CONSO_MSISDN_DAY WHERE EVENT_DATE between date_sub('###SLICE_VALUE###', 29) and '###SLICE_VALUE###') B
