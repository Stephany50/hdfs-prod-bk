SELECT IF(A.FT_EXIST = 0 and B.FT_CRA_GPRS_EXIST>0 and C.FT_CRA_GPRS_POST_EXIST>0, "OK", "NOK") FROM
(SELECT count(*) FT_EXIST FROM MON.SPARK_FT_MSISDN_IMEI_DATA_LOCATION WHERE SESSION_DATE='###SLICE_VALUE###') A,
(SELECT COUNT(*) FT_CRA_GPRS_EXIST FROM MON.SPARK_FT_CRA_GPRS WHERE SESSION_DATE='###SLICE_VALUE###') B,
(SELECT COUNT(*) FT_CRA_GPRS_POST_EXIST FROM MON.SPARK_FT_CRA_GPRS_POST WHERE SESSION_DATE='###SLICE_VALUE###') C
