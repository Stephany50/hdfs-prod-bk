SELECT IF(A.FT_EXIST = 0 and B.FT_COUNT>0 and C.IT_COUNT>0 , "OK","NOK") FROM
(SELECT COUNT(*) FT_EXIST FROM MON.SPARK_FT_BYPASS_ACCOUNT_CREDIT WHERE EVENT_DATE='###SLICE_VALUE###') A,
(SELECT COUNT(*) FT_COUNT FROM MON.SPARK_FT_CONTRACT_SNAPSHOT WHERE EVENT_DATE = '###SLICE_VALUE###') B,
(SELECT COUNT(*) IT_COUNT FROM CDR.SPARK_IT_SIMBOXDETECTION WHERE EVENT_DATE= '###SLICE_VALUE###') C
