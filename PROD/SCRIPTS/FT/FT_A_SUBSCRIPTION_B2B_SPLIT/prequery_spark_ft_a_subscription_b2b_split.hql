SELECT IF(T_1.FT_COUNT = 0 AND T_2.FT_COUNT > 0 ,"OK","NOK") FT_EXIST
FROM
(SELECT COUNT(*) FT_COUNT FROM MON.SPARK_FT_A_SUBSCRIPTION_B2B_SPLIT WHERE TRANSACTION_DATE='###SLICE_VALUE###') T_1,
(SELECT COUNT(*) FT_COUNT FROM mon.spark_ft_subscription WHERE transaction_date ='###SLICE_VALUE###') T_2

