SELECT IF(TT_NOT_EXISTS = 0 and BILL_EXISTS>0 ,"OK","NOK") TABLES_EXIST
FROM
(SELECT COUNT(*) TT_NOT_EXISTS FROM MON.SPARK_TT_PICO_KPIS WHERE event_month='###SLICE_VALUE###' and KPI='CALLS_ON_N') A,
(SELECT COUNT(*) BILL_EXISTS FROM MON.spark_ft_billed_transaction_prepaid WHERE transaction_date='###SLICE_VALUE###-01') B