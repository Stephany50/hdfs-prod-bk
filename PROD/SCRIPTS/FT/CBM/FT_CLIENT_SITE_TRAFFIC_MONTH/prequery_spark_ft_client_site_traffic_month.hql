SELECT IF(
    A.FT_EXIST = 0
    AND B.FT_COUNT > DATEDIFF(LAST_DAY('###SLICE_VALUE###'||'-01') , '###SLICE_VALUE###'||'-01') , "OK","NOK"
) FROM
(SELECT COUNT(*) FT_EXIST FROM MON.SPARK_FT_CLIENT_SITE_TRAFFIC_MONTH WHERE EVENT_MONTH='###SLICE_VALUE###') A,
(SELECT COUNT(DISTINCT EVENT_DATE) FT_COUNT FROM MON.SPARK_FT_CLIENT_SITE_TRAFFIC_DAY WHERE EVENT_DATE BETWEEN TO_DATE('###SLICE_VALUE###'||'-01') AND LAST_DAY('###SLICE_VALUE###'||'-01')) B