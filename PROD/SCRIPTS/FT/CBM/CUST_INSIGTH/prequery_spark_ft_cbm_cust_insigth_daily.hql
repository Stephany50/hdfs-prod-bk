SELECT IF(T_1.FT_EXIST = 0 and T_2.FT_BILLED_TRANSACTION_PREPAID>0 and T_3.FT_MSC_TRANSACTION>0 and T_4.FT_CRA_GPRS>0 ,"OK","NOK") IT_CTI_EXIST
FROM
(SELECT COUNT(*) FT_EXIST FROM MON.SPARK_FT_CBM_CUST_INSIGTH_DAILY WHERE PERIOD = '###SLICE_VALUE###') T_1,
(SELECT COUNT(*) FT_BILLED_TRANSACTION_PREPAID FROM MON.SPARK_FT_BILLED_TRANSACTION_PREPAID WHERE TRANSACTION_DATE = '###SLICE_VALUE###') T_2,
(SELECT COUNT(*) FT_MSC_TRANSACTION FROM MON.FT_MSC_TRANSACTION WHERE TRANSACTION_DATE = '###SLICE_VALUE###') T_3,
(SELECT COUNT(*) FT_CRA_GPRS FROM MON.SPARK_FT_CRA_GPRS WHERE SESSION_DATE = '###SLICE_VALUE###') T_4
