create table MON.SPARK_FT_MSISDN_PREMIUM_MONTH
(
    MSISDN VARCHAR(50),
    REVENU_MOYEN DOUBLE,
    PREMIUM DOUBLE,
    CONSO_MOY_DATA DOUBLE,
    RECHARGE_MOY DOUBLE,
    PREMIUM_PLUS DOUBLE,
    INSERT_DATE TIMESTAMP
)
PARTITIONED BY (EVENT_MONTH VARCHAR(10))
 STORED AS PARQUET TBLPROPERTIES ('PARQUET.COMPRESS'='SNAPPY')
