    , SOURCE_TABLE
    , OPERATOR_CODE
    --, INSERT_DATE
    --, THROTTLING_VOL
    , EVENT_DATE
    , COUNT(*) COUNTER
FROM (
    SELECT DISTINCT
        MSISDN
        , COMMERCIAL_OFFER
        , BYTES_SENT
        , BYTES_RECEIVED
        , MMS_COUNT
        --, MAIN_RATED_AMOUNT
