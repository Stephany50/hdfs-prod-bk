    , COMMERCIAL_OFFER
    , BYTES_SENT
    , BYTES_RECEIVED
    , MMS_COUNT
    --, MAIN_RATED_AMOUNT
    , PROMO_RATED_AMOUNT
    , SERVED_PARTY_IMSI
    , SERVED_PARTY_IMEI
    , BYTES_USED_IN_BUNDLE
    --, BYTES_USED_OUT_BUNDLE
