    , GOS_DEBIT_COUNT
    , GOS_SESSION_COUNT
    , GOS_REFUND_COUNT
    , GOS_DEBIT_AMOUNT
    , GOS_SESSION_AMOUNT
    , GOS_REFUND_AMOUNT
