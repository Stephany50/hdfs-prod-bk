SELECT IF(A.COUNT_FT_VAS_REVENUE_DETAIL = 0 and B.COUNT_FT_BILLED_TRANSACTION_PREPAID>0,"OK","NOK") FROM
    (SELECT count(*) COUNT_FT_VAS_REVENUE_DETAIL FROM MON.SPARK_FT_VAS_REVENUE_DETAIL WHERE TRANSACTION_DATE='###SLICE_VALUE###' LIMIT 10) A,
    (SELECT COUNT(*) COUNT_FT_BILLED_TRANSACTION_PREPAID FROM MON.SPARK_FT_BILLED_TRANSACTION_PREPAID WHERE TRANSACTION_DATE='###SLICE_VALUE###' LIMIT 10) B

