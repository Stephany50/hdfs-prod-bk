SELECT IF(A.FT_EXIST = 0 and B.FT_MSC_TRANSACTION_EXIST>0, "OK", "NOK") FROM
(SELECT count(*) FT_EXIST FROM MON.SPARK_FT_CLIENT_LAC_TRAFFIC_DAY WHERE EVENT_DATE='###SLICE_VALUE###') A,
(SELECT COUNT(*) FT_MSC_TRANSACTION_EXIST FROM MON.SPARK_FT_MSC_TRANSACTION WHERE TRANSACTION_DATE='###SLICE_VALUE###') B
