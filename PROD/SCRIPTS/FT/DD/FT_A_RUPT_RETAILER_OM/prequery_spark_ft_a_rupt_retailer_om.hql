SELECT IF(A.NB_EVENT_TIME<B.NB_EVENT_TIME , "OK", "NOK")
FROM
(SELECT COUNT(DISTINCT EVENT_TIME) NB_EVENT_TIME FROM DD.SPARK_FT_A_RUPT_RETAILER_OM WHERE EVENT_DATE = '###SLICE_VALUE###') A,
(SELECT COUNT(DISTINCT EVENT_TIME) NB_EVENT_TIME FROM DD.SPARK_FT_RUPT_RETAILER_OM WHERE EVENT_DATE = '###SLICE_VALUE###') B