INSERT INTO DD.SPARK_FT_A_RUPT_RETAILER_OM
SELECT
  PARTENAIRE,
  SITE_NAME,
  COUNT(DISTINCT MOBILE_NUMBER) MSISDN_COUNT,
  SUM(RUPTURE_HOUR_MSISDN) NBRE_RUPT,
  CURRENT_TIMESTAMP() INSERT_DATE,
  EVENT_DATE,
  EVENT_TIME
FROM DD.SPARK_FT_RUPT_RETAILER_OM
WHERE EVENT_DATE='###SLICE_VALUE###' AND EVENT_TIME NOT IN ( SELECT DISTINCT EVENT_TIME FROM DD.SPARK_FT_A_RUPT_RETAILER_OM WHERE EVENT_DATE='###SLICE_VALUE###')
GROUP BY EVENT_DATE,
  EVENT_TIME,
  PARTENAIRE,
  SITE_NAME