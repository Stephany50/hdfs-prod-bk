SELECT IF(A.COUNT_FT = 0 AND B.COUNT_CLSD>0 AND C.COUNT_BDI>0 AND D.COUNT_MSC=90,"OK","NOK") FROM
(SELECT count(*) COUNT_FT FROM MON.SPARK_FT_MULTI_SIM WHERE EVENT_DATE=CURRENT_DATE()) A,
(SELECT COUNT(*) COUNT_CLSD FROM MON.SPARK_FT_CLIENT_LAST_SITE_DAY WHERE EVENT_DATE=DATE_SUB(CURRENT_DATE(), 3)) B,
(SELECT COUNT(*) COUNT_BDI FROM CDR.SPARK_IT_KYC_BDI_FULL WHERE ORIGINAL_FILE_DATE=DATE_SUB(CURRENT_DATE(), 3)) C,
(SELECT COUNT(DISTINCT TRANSACTION_DATE) COUNT_MSC FROM MON.SPARK_FT_MSC_TRANSACTION WHERE TRANSACTION_DATE BETWEEN DATE_SUB(CURRENT_DATE(), 92) AND DATE_SUB(CURRENT_DATE(), 3)) D
 