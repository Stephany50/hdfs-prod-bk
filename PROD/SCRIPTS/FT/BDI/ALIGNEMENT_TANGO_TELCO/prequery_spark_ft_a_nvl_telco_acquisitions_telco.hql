SELECT IF(A.NBR = 0 AND B.NBR >0 AND C.NBR >0 AND D.NBR >0 ,"OK","NOK") FROM
(SELECT COUNT(*) NBR FROM AGG.SPARK_FT_A_NVL_ACQUISITIONS_TELCO WHERE EVENT_DATE= '###SLICE_VALUE###') A,
(SELECT COUNT(*) NBR FROM MON.SPARK_FT_ALIGNEMENT_TANGO_TELCO WHERE EVENT_DATE= '###SLICE_VALUE###') B,
(select count(*) as NBR from MON.BACKUP_SPARK_DT_BASE_IDENTIFICATION where processing_date='###SLICE_VALUE###') C,
(SELECT COUNT(*) NBR FROM MON.SPARK_FT_BDI WHERE EVENT_DATE= '###SLICE_VALUE###') D

