SELECT IF(
T_1.FT_EXIST = 0 AND
T_2.ACTIVATIONS_SITE_DAY>0
,"OK","NOK") ACT_MONT_EXIST
FROM
(SELECT COUNT(*) FT_EXIST FROM MON.SPARK_FT_PARC_ACTIVATIONS_SITE WHERE TO_DATE(EVENT_DATE) = '###SLICE_VALUE###') T_1,
(SELECT COUNT(*) ACTIVATIONS_SITE_DAY FROM MON.SPARK_FT_ACTIVATIONS_SITE_DAY WHERE TO_DATE(ACTIVATION_DATE) = '###SLICE_VALUE###') T_2