CREATE TABLE MON.SPARK_FT_SUBSCRIPTION_ACCESS
(
   NUM_COMPTE VARCHAR(50),
    INTITULE_STRUCTURE VARCHAR(75),
    MSISDN VARCHAR(25),
    TYPE_FORFAIT VARCHAR(40),
    MONTANT_TTC BIGINT,
    INSERT_DATE  TIMESTAMP
)
PARTITIONED BY (TRANSACTION_DATE DATE)
STORED AS PARQUET
TBLPROPERTIES ('PARQUET.COMPRESS'='SNAPPY');