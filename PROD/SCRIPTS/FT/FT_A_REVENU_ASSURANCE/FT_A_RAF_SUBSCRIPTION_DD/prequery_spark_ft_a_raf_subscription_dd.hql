SELECT IF(T_1.FT_A_RAF_SUBSCRIPTION_DD = 0 and T_2.FT_SUBSCRIPTION > 1,"OK","NOK") FT_EXIST
FROM
    (SELECT COUNT(*) FT_A_RAF_SUBSCRIPTION_DD FROM AGG.SPARK_FT_A_RAF_SUBSCRIPTION_DD WHERE EVENT_DATE='###SLICE_VALUE###') T_1,
    (SELECT COUNT(*) FT_SUBSCRIPTION FROM MON.SPARK_FT_SUBSCRIPTION WHERE TRANSACTION_DATE='###SLICE_VALUE###') T_2