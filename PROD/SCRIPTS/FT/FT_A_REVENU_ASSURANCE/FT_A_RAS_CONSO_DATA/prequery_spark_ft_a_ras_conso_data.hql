SELECT IF(T_1.FT_A_RAS_CONSO_DATA = 0 and T_2.FT_DATA_CONSO_MSISDN_DAY > 1,"OK","NOK") FT_EXIST
FROM
    (SELECT COUNT(*) FT_A_RAS_CONSO_DATA FROM AGG.SPARK_FT_A_RAS_CONSO_DATA WHERE EVENT_DATE='###SLICE_VALUE###') T_1,
    (SELECT COUNT(*) FT_DATA_CONSO_MSISDN_DAY FROM MON.SPARK_FT_DATA_CONSO_MSISDN_DAY WHERE EVENT_DATE='###SLICE_VALUE###') T_2