SELECT IF(T_1.FT_MSC_CMR_BAD_CALL = 0 and T_2.FT_MSC_TRANSACTION > 1,"OK","NOK") FT_EXIST
FROM
    (SELECT COUNT(*) FT_MSC_CMR_BAD_CALL FROM MON.SPARK_FT_MSC_CMR_BAD_CALL WHERE TRANSACTION_DATE='###SLICE_VALUE###') T_1,
    (SELECT COUNT(*) FT_MSC_TRANSACTION FROM MON.SPARK_FT_MSC_TRANSACTION WHERE TRANSACTION_DATE='###SLICE_VALUE###') T_2