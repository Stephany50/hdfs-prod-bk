SELECT IF(T_1.FT_A_RAS_RECHARGE_2 = 0 and T_2.FT_RECHARGE > 1,"OK","NOK") FT_EXIST
FROM
    (SELECT COUNT(*) FT_A_RAS_RECHARGE_2 FROM AGG.SPARK_FT_A_RAS_RECHARGE_2 WHERE EVENT_DATE='###SLICE_VALUE###') T_1,
    (SELECT COUNT(*) FT_RECHARGE FROM MON.SPARK_FT_REFILL WHERE REFILL_DATE='###SLICE_VALUE###') T_2