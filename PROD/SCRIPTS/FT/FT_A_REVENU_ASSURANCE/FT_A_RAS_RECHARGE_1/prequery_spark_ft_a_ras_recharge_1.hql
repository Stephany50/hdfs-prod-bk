SELECT IF(T_1.FT_A_RAS_RECHARGE_1 = 0 and T_2.FT_RECHARGE > 1,"OK","NOK") FT_EXIST
FROM
    (SELECT COUNT(*) FT_A_RAS_RECHARGE_1 FROM AGG.SPARK_FT_A_RAS_RECHARGE_1 WHERE EVENT_DATE='###SLICE_VALUE###') T_1,
    (SELECT COUNT(*) FT_RECHARGE FROM MON.SPARK_FT_RECHARGE WHERE TRANSACTION_DATE='###SLICE_VALUE###') T_2