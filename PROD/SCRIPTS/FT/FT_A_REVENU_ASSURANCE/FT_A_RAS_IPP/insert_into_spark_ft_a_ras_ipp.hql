INSERT INTO AGG.SPARK_FT_A_RAS_IPP
SELECT A.TRANSACTION_DATE,
A.SUBSCRIPTION_SERVICE_DETAILS,
COUNT(A.SERVED_PARTY_MSISDN) AS NBRE_OCCURENCE,
CURRENT_TIMESTAMP AS INSERT_DATE,
'###SLICE_VALUE###' EVENT_DATE
FROM MON.SPARK_FT_SUBSCRIPTION  A
INNER JOIN MON.SEVERAL_SUBSCRIPTIONS B
ON A.SUBSCRIPTION_SERVICE_DETAILS = TRIM(B.SUBSCRIPTION_SERVICE_DETAILS)
WHERE A.TRANSACTION_DATE = '###SLICE_VALUE###'
AND B.EVENT_DATE = '###SLICE_VALUE###'
GROUP BY A.TRANSACTION_DATE, A.SUBSCRIPTION_SERVICE_DETAILS