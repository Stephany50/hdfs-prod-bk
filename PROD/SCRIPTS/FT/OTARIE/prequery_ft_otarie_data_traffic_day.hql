SELECT IF(T_2.FT_EXIST = 0 and T_3.CONTRACT_SNAPSHOT_EXIST>0 and T_1.COMPLETUDE>0,"OK","NOK") EXTRACT_EXIST
FROM
(SELECT COUNT(*) COMPLETUDE FROM CDR.IT_TRAFFIC_CUST_OTARIE WHERE TRANSACTION_DATE='###SLICE_VALUE###') T_1,
(SELECT COUNT(*) FT_EXIST FROM MON.FT_OTARIE_DATA_TRAFFIC_DAY WHERE TRANSACTION_DATE='###SLICE_VALUE###') T_2,
(SELECT COUNT(*) CONTRACT_SNAPSHOT_EXIST FROM MON.FT_CONTRACT_SNAPSHOT WHERE EVENT_DATE = '###SLICE_VALUE###') T_3
