SELECT IF( MSC_EXISTS >=90 AND FT_EXISTS=0,'OK','NOK') FROM
(SELECT COUNT(*) FT_EXISTS FROM MON.FT_PDM_TRANSACTION_OTHER WHERE EVENT_DATE = '2019-01-01') T,
(SELECT COUNT(*) MSC_EXISTS FROM (SELECT TRANSACTION_DATE  FROM MON.FT_MSC_TRANSACTION
    WHERE TRANSACTION_DATE BETWEEN DATE_SUB('2019-01-01',89) AND '2019-01-01' GROUP BY TRANSACTION_DATE) a) T1