SELECT IF(T_1.FT_EXIST_PREC > 0 and T_2.FT_EXIST = 0,"OK","NOK") EXTRACT_EXIST
FROM
    (SELECT COUNT(*) FT_EXIST_PREC FROM MON.SPARK_FT_SVA_SIM_BACKUP WHERE EVENT_DATE=date_sub('###SLICE_VALUE###',1)) T_1,
    (SELECT COUNT(*) FT_EXIST FROM MON.SPARK_FT_SVA_SIM_BACKUP WHERE EVENT_DATE='###SLICE_VALUE###') T_2
