
INSERT INTO MON.SPARK_FT_MSC_CALL_CENTER
SELECT
    TRANSACTION_TIME,
    SERVED_MSISDN,
    OTHER_PARTY,
    TRANSACTION_TYPE,
    TRANSACTION_DIRECTION,
    CURRENT_TIMESTAMP() INSERT_DATE,
    TRANSACTION_DATE
FROM MON.SPARK_FT_MSC_TRANSACTION WHERE TRANSACTION_DATE='###SLICE_VALUE###'
AND OTHER_PARTY IN ( '8900', '8911','96400400','699900900','699900555','23341000')