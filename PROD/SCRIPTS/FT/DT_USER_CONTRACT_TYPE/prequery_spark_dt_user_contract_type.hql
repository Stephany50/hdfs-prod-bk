SELECT IF(A.FT_EXIST = 0 AND B.IT_EXIST>0, 'OK', 'NOK') FROM
(
    SELECT COUNT(*) FT_EXIST
    FROM DIM.SPARK_DT_USER_CONTRACT_TYPE
    WHERE PROCESSING_DATE BETWEEN DATE_SUB('###SLICE_VALUE###', ${hivevar:days_interval}) AND '###SLICE_VALUE###'
) A,
(SELECT COUNT(*) IT_EXIST FROM CDR.SPARK_IT_ACCOUNT WHERE ORIGINAL_FILE_DATE = '###SLICE_VALUE###') B
