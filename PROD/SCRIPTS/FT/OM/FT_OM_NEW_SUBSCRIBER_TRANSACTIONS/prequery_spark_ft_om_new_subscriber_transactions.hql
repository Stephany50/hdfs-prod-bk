SELECT IF(T_1.FT_COUNT = 0 AND T_2.IT_OM_SUBSCRIBERS > 0 AND T_3.IT_OMNY_TRANSACTIONS>0,"OK","NOK") FT_EXIST
FROM
(SELECT COUNT(*) FT_COUNT FROM MON.SPARK_FT_OM_NEW_SUBSCRIBER_TRANSACTIONS WHERE EVENT_DATE='###SLICE_VALUE###') T_1,
(SELECT COUNT(*) IT_OM_SUBSCRIBERS FROM CDR.SPARK_IT_OM_SUBSCRIBERS WHERE MODIFICATION_DATE='###SLICE_VALUE###') T_2,
(SELECT COUNT(*) IT_OMNY_TRANSACTIONS FROM CDR.SPARK_IT_OMNY_TRANSACTIONS WHERE TRANSFER_DATETIME='###SLICE_VALUE###') T_3