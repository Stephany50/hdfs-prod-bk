CREATE TABLE MON.SPARK_FT_DATAMART_OM_MARKETING_OLD(
    MSISDN VARCHAR(12),
    USER_ID VARCHAR(50),
    SITE_NAME VARCHAR(200),
    SERVICE_TYPE VARCHAR(50),
    VOL DOUBLE,
    VAL DOUBLE,
    COMMISSION DOUBLE,
    REVENU DOUBLE,
    STYLE VARCHAR(100),
    DETAILS VARCHAR(100)
)                        
PARTITIONED BY (JOUR  DATE)
STORED AS PARQUET TBLPROPERTIES ('PARQUET.COMPRESS=SNAPPY')