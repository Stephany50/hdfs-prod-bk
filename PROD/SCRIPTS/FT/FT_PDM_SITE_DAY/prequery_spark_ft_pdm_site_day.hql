SELECT IF(T_1.IT_COUNT= 0 AND T_2.FT_OTHER_PARTY_SITE_TRAFFIC> 0 AND T_3.FT_OTHER_PARTY_SITE_TRAFFIC>0 ,"OK","NOK") SITE_DAY_EXIST
FROM
    (SELECT COUNT(*) IT_COUNT FROM MON.SPARK_FT_PDM_SITE_DAY WHERE START_DATE='###SLICE_VALUE###') T_1,
    (SELECT COUNT(*) FT_OTHER_PARTY_SITE_TRAFFIC FROM MON.SPARK_FT_OTHER_PARTY_SITE_TRAFFIC WHERE EVENT_DATE='###SLICE_VALUE###') T_2,
    (SELECT COUNT(*) FT_OTHER_PARTY_SITE_TRAFFIC FROM MON.SPARK_FT_PDM_OCM_SITE_DAY WHERE EVENT_DATE='###SLICE_VALUE###') T_3