SELECT IF(T_1.FT_EXIST = 0 and T_2.IT_EXIST > 0 and T_3.IT_EXIST_1 > 0 ,"OK","NOK") FT_SUBS_EXIST_B2B
FROM
(SELECT COUNT(*) FT_EXIST FROM MON.SPARK_FT_REPORTING_DIMELO WHERE EVENT_DATE = '###SLICE_VALUE###') T_1,
(SELECT COUNT(*) IT_EXIST FROM CDR.SPARK_IT_DIMELO_JOURNAL WHERE FILE_DATE = '###SLICE_VALUE###') T_2,
(SELECT COUNT(*) IT_EXIST_1 FROM CDR.SPARK_IT_DIMELO_AGENTS  WHERE FILE_DATE = '###SLICE_VALUE###') T_3
--(SELECT COUNT(*) IT_EXIST_2 FROM CDR.SPARK_IT_DIMELO_SOURCES WHERE FILE_DATE = '###SLICE_VALUE###') T_4