SELECT IF(
    T_1.ft_tab = 0
    AND T_2.ft_tab > 0
    AND T_3.ft_tab > 0
    , 'OK'
    , 'NOK'
)
FROM
(SELECT COUNT(*) ft_tab FROM MON.SPARK_FT_FMS_MSC_OM WHERE event_date = '###SLICE_VALUE###') T_1,
(SELECT COUNT(*) ft_tab FROM CDR.SPARK_IT_OMNY_ACCOUNT_SNAPSHOT_NEW WHERE original_file_date = '###SLICE_VALUE###') T_2,
(SELECT COUNT(*) ft_tab FROM MON.SPARK_FT_MSC_TRANSACTION WHERE TRANSACTION_DATE = '###SLICE_VALUE###') T_3