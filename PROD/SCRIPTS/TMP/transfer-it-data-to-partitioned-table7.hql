select *
FROM
(SELECT COUNT(*) FT_EXIST FROM MON.SPARK_FT_CONTRACT_SNAPSHOT WHERE EVENT_DATE = '2021-03-21') A,
(SELECT COUNT(*) PRE_FT_EXIST FROM MON.SPARK_FT_CONTRACT_SNAPSHOT WHERE EVENT_DATE = DATE_SUB(TO_DATE(FROM_UNIXTIME(UNIX_TIMESTAMP('2021-03-21', 'yyyy-MM-dd'))), 1)) B,
(SELECT COUNT(*) ACC_NBR_EXIST FROM CDR.SPARK_IT_ZTE_ACC_NBR_EXTRACT WHERE ORIGINAL_FILE_DATE = '2021-03-21') C,
(SELECT COUNT(*) SUBS_EXIST FROM CDR.SPARK_IT_ZTE_SUBS_EXTRACT WHERE ORIGINAL_FILE_DATE = '2021-03-21') D,
(SELECT COUNT(*) PROD_EXIST FROM CDR.SPARK_IT_ZTE_PROD_EXTRACT WHERE ORIGINAL_FILE_DATE = '2021-03-21') E,
(SELECT COUNT(*) PRICE_PLAN_EXIST FROM CDR.SPARK_IT_ZTE_PRICE_PLAN_EXTRACT WHERE ORIGINAL_FILE_DATE = '2021-03-21') F,
(SELECT COUNT(*) ACCT_EXIST FROM CDR.SPARK_IT_ZTE_ACCT_EXTRACT WHERE ORIGINAL_FILE_DATE = '2021-03-21') G,
(SELECT COUNT(*) BAL_EXIST FROM CDR.SPARK_IT_ZTE_BAL_EXTRACT WHERE ORIGINAL_FILE_DATE = '2021-03-21') H,
(SELECT COUNT(*) CUST_EXIST FROM CDR.SPARK_IT_ZTE_CUST_EXTRACT WHERE ORIGINAL_FILE_DATE = '2021-03-21') I
