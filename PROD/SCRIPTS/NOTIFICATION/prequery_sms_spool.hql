SELECT  IF(B_1.SMS_SPOOL_EXISTS = 0 AND T_1.log_date_exist > 0 ,"OK","NOK") SMS_SPOOL
FROM
(SELECT COUNT(*) SMS_SPOOL_EXISTS FROM  MON.SMS_SPOOL WHERE TRANSACTION_DATE='###SLICE_VALUE###') B_1,
(SELECT COUNT(*) log_date_exist FROM CDR.IT_LOG WHERE log_date= '###SLICE_VALUE###' AND fluxtype IN ('OUT_SFTP','OUT_FTP')) T_1