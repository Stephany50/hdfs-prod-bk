SELECT IF(A.DATE_IS_PROCESSING=0  AND B.ZEBRA_MASTER>0 AND C.PROCESSING_DATE_PREV>0,'OK','NOK' ) STATUS
FROM
(SELECT COUNT(*) DATE_IS_PROCESSING  FROM MON.BACKUP_SPARK_DIM_STK_HIERACHY WHERE EVENT_DATE='###SLICE_VALUE###') A,
(SELECT COUNT(*) ZEBRA_MASTER FROM CDR.SPARK_IT_ZEBRA_MASTER WHERE TRANSACTION_DATE = '###SLICE_VALUE###') B,
(SELECT COUNT(*) PROCESSING_DATE_PREV  FROM MON.BACKUP_SPARK_DIM_STK_HIERACHY WHERE EVENT_DATE=DATE_SUB('###SLICE_VALUE###',1)) C
